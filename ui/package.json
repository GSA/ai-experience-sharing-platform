  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test",
    "test:pa11y": "pa11y-ci",
    "eject": "react-scripts eject",
    "test:coverage": "npm test -- --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "jsx-a11y/anchor-is-valid": "off",
      "jsx-a11y/role-supports-aria-props": "off",
      "array-callback-return": "off",
      "import/no-anonymous-default-export": [
        "error",
        {
          "allowArray": false,
          "allowArrowFunction": true,
          "allowAnonymousClass": false,
          "allowAnonymousFunction": true,
          "allowCallExpression": true,
          "allowLiteral": false,
          "allowObject": false
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.js",
      "src/app/**/*.js",
      "src/features/**/*.js",
      "src/routes/**/*.js",
      "src/templates/**/*.js",
      "!src/app/index.js",
      "!src/**/context/**/*.js",
      "!src/test/**/*.js",
      "!src/routes/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "immer": ">=9.0.6",
    "is-svg": ">=4.2.2",
    "node-notifier": ">=8.0.1",
    "npm-force-resolutions": "0.0.10",
    "object-path": ">=0.11.8",
    "pa11y-ci": "^2.4.0",
    "postcss": "^7.0.36",
    "ssri": ">=8.0.1"
  },
  "resolutions": {
    "hast-util-sanitize": "git+https://github.com/flexion/hast-util-sanitize.git#a6f5e00526490acce75651b18fc5a7027be9597b",
    "trim": "0.0.3",
    "nth-check": "^2.0.1",
    "@babel/core": "7.12.9",
    "@babel/traverse": "7.14.2",
    "@babel/types": "7.14.4",
    "@babel/generator": "7.14.3",
    "@babel/parser": "7.14.3",
    "normalize-url": "4.5.1",
    "trim-newlines": "3.0.1",
    "glob-parent": "5.1.2",
    "browserslist": "4.16.5"
  }
